<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ResultEllipse" TargetType="Ellipse">
        <Setter Property="Height" Value="100"/>
        <Setter Property="Width" Value="100"/>
        <Setter Property="Fill" Value="{DynamicResource ThemeBrush}"/>
        <Setter Property="Stroke" Value="{DynamicResource ControlForegroundBrush}"/>
        <Setter Property="StrokeThickness" Value="2"/>
    </Style>
    
    <Style x:Key="ResultRectangle" TargetType="Rectangle">
        <Setter Property="Height" Value="5"/>
        <Setter Property="Width" Value="75"/>
        <Setter Property="Margin" Value="5,0"/>
        <Setter Property="Fill" Value="{DynamicResource ControlForegroundBrush}"/>
    </Style>
</ResourceDictionary>